datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  createdAt         DateTime                  @default(now())
  description       String?
  id                String                    @id @default(cuid())
  priority          EnumTicketPriority?
  status            EnumTicketStatus?
  ticketDescription String?
  ticketPriority    EnumTicketTicketPriority?
  ticketStatus      EnumTicketTicketStatus?
  ticketTitle       String?
  title             String?
  updatedAt         DateTime                  @updatedAt
}

enum EnumTicketPriority {
  Option1
}

enum EnumTicketStatus {
  Option1
}

enum EnumTicketTicketPriority {
  Option1
}

enum EnumTicketTicketStatus {
  Option1
}
